// write a program to get no from user and find equal them using assignment opertor print it on console
#include <stdio.h>
int getNumberFormUser(){
    int i;
    scanf("%d",&i);
    return i;
}
int findEqualValue(int a,int b){
    return a==b;
}

int main() {
      printf("Enter a first value ");
      int number1 = getNumberFormUser();
    printf("Enter A Second value ");
       int number2 = getNumberFormUser();
int equal = findEqualValue(number1,number2);
  printf("value is %d",equal);
    return 0;
}
