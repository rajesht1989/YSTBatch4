// write a program to get no from user and divided them using assignment opertor print it on console
#include <stdio.h>
int getNumberFormUser(){
    int i;
    scanf("%d",&i);
    return i;
}
int findDividedThemValue(int a,int b){
    return a/=b;
}

int main() {
      printf("Enter a first value ");
      int number1 = getNumberFormUser();
    printf("Enter A Second value ");
       int number2 = getNumberFormUser();
int divided = findDividedThemValue(number1,number2);
  printf("%d",divided);
    return 0;
}
