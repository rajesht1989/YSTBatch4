// write a program to get no from useer and print simple star print on console
#include <stdio.h>
int getNoFromUser(){
    int i;
     printf ("Enter A No ");
    scanf("%d",&i);
    return i;
}
int printStar(int  a){
  for (int i = 0 ; i < a;i++){
      for (int i = 0 ; i < a  ;i++){
       printf ("*\t");
   }
    printf ("\n");
  }
   
}
int main(){
   
     int number1 = getNoFromUser();
   printStar(number1);
    return 0;
}
