//Get a two num from user &print it on console
#include<stdio.h>
void main() 
{
    int num1= getATwoNumberFromUser();
    int num2= getATwoNumberFromUser();
    printf("Number is%d,%d",num1,num2);
}
int getATwoNumberFromUser() 
{
    int i;
    printf("Enter a number=");
    scanf("%d",&i);
    return i;
}
