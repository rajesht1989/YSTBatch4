// write a program to get no from user and find not gate equal value using logical opertor print it on console
#include <stdio.h>
int getNumberFormUser(){
    int i;
    scanf("%d",&i);
    return i;
}
int findNotGateValue(int a,int b){
    return a!b;
}

int main() {
      printf("Enter a first value ");
      int number1 = getNumberFormUser();
    printf("Enter A Second value ");
       int number2 = getNumberFormUser();
int notgate = findNotGateValue(number1,number2);
  printf("value is %d",notgate);
    return 0;
}
