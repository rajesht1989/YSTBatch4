//Get a three decimals num from user &print it on console
#include<stdio.h>
void main() 
{
    float num1= getAThreeNumberFromUser();
    float num2= getAThreeNumberFromUser();
    float num3= getAThreeNumberFromUser();
    printf("Numbers are %f,%f,%f",num1,num2,num3);
}
int getAThreeNumberFromUser() 
{
    float i;
    printf("Enter a number=");
    scanf("%f",&i);
    return i;
}
