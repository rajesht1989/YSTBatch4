//Get a three num from user &print it on console
#include<stdio.h>
void main() 
{
    int num1= getAThreeNumberFromUser();
    int num2= getAThreeNumberFromUser();
    int num3= getAThreeNumberFromUser() ;
    printf("Numbers are %d,%d,%d",num1,num2,num3);
}
int getAThreeNumberFromUser() 
{
    int i;
    printf("Enter a number=");
    scanf("%d",&i);
    return i;
}
